st [hamburger, setHamburger] = useState(false);\r\n\r\n  const [sort, setSort] = useState(\"\");\r\n\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(() => {\r\n    dispatch(listProducts());\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (products) {\r\n      dispatch(filterProducts(products, def, low, high, speed, power, color));\r\n    }\r\n  }, [def, low, high, speed, power, color]);\r\n\r\n  useEffect(() => {\r\n    setDisplayedProducts(filteredProducts ? filteredProducts : products);\r\n  }, [products, filteredProducts]);\r\n\r\n  return (\r\n    <div className=\"shoppage\">\r\n      <aside\r\n        className={\r\n          hamburger ? \"hamburger hamburgeropen\" : \"hamburger hamburgerclosed\"\r\n        }>\r\n        <div id=\"xparent\">\r\n          <div id=\"x\">\r\n            <Icon\r\n              onClick={() => setHamburger(false)}\r\n              icon={closeIcon}\r\n              style={{ color: \"#393636\", fontSize: \"24px\" }}\r\n            />\r\n          </div>\r\n        </div>\r\n        <div id=\"content\">\r\n          <div id=\"categories\">\r\n            <h3>Categories</h3>\r\n            <div id=\"filter-categories\">\r\n              <select\r\n                onChange={(e) => {\r\n                  setDef(e.target.value);\r\n                }}>\r\n                <option value=\"\">Select categories</option>\r\n                <option value=\"E-Scooter\">E-scooter</option>\r\n                <option value=\"Kick Stand\">Kick Stand</option>\r\n                <option value=\"Helmet\">Helmet</option>\r\n              </select>\r\n            </div>\r\n            <h3>Sort By</h3>\r\n            <div className=\"filter-sort\">\r\n              <select\r\n                onChange={(e) => {\r\n                  setSort(e.target.value);\r\n                }}>\r\n                <option value=\"\">Sort By</option>\r\n                <option value=\"lowest\">Lowest First</option>\r\n                <option value=\"highest\">Highest